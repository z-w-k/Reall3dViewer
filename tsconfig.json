{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "lib": ["ES2020", "ES2021", "DOM", "DOM.Iterable"],
        "moduleResolution": "bundler",
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": ["vite-plugin-glsl/ext"],
        "outDir": "./dist"
    },
    "include": ["src"]
}
