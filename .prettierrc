{
    "printWidth": 150,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false, 
    "trailingComma": "all",
    "bracketSpacing": true, 
    "jsxBracketSameLine": false,
    "arrowParens": "avoid", 
    "rangeStart": 0,
    "requirePragma": false,
    "insertPragma": false, 
    "proseWrap": "preserve", 
    "htmlWhitespaceSensitivity": "css", 
    "vueIndentScriptAndStyle": false, 
    "endOfLine": "auto",
    "embeddedLanguageFormatting": "auto", 
    "singleAttributePerLine": false,
    "wrapAttributes": true
  }
  